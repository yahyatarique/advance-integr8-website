{
  "scripts": {
    "build-css": "npx tailwindcss -i ./src/input.css -o ./build/static/output.css --watch",
    "dev": "live-server ./build",
    "build-netlify": "npx tailwindcss -i ./src/input.css -o ./build/static/output.css --minify",
    "push-to-test": "git status && git add . && git commit -m 'pushed-through-cli' && git push origin develop"
  },
  "dependencies": {
    "gsap": "^3.12.2",
    "tailwindcss": "^3.3.5"
  },
  "devDependencies": {
    "live-server": "^1.2.2",
    "minify": "^10.5.2"
  }
}
